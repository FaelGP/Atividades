#include <stdio.h>

main() {
    
    int A; 
    int B; 
    int C; 
    int D; 
    int diferenca;
    
    printf("Digite o quanto vale  A: ");
    scanf("%d", &A);
    
    printf("Digite o quanto vale B: ");
    scanf("%d", &B);
    
    printf("Digite o quanto vale C: ");
    scanf("%d", &C);
    
    printf("Digite o quanto vale D: ");
    scanf("%d", &D);
    
    diferenca = A * B - C * D;
    
    printf("diferenca = (%d * %d - %d * %d)\n", A, B, C, D);
    printf("diferenca = %d\n", diferenca);
    
    return 0;
}
