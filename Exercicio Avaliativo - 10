#include <stdio.h>

int main() {
    int num, soma = 0;
    float media;
    for (int i = 1; i <= 10; i++) {
        printf("Digite o %do número: ", i);
        scanf("%d", &num);
        soma += num;
    }
    media = (float) soma / 10;
    printf("A média dos 10 números informados é: %.2f\n", media);
    return 0;
}
