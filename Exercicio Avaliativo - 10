#include <stdio.h>

main() {
    
    int num;
    int soma = 0;
    
    float media;
    
    for (int i = 1; i <= 10; i++) 
    {
        
        printf("Digite o %d do número: ", num_int);
        scanf("%d", &num);
        
        soma += num;
    }
    media = (float) soma / 10;
    
    printf("A média dos 10 números que foram informados é de: %.2f\n", media);
    
    return 0;
}
