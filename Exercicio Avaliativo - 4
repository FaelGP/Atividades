#include <stdio.h>

main() {
    
    float x; 
    float y;
    
    printf("Digite as coordenadas dos pontos (x,y):\n ");
    scanf("%f \n %f", &x, &y);

    if (x == 0 && y == 0) {
        
        printf("Origem dos pontos\n");
    
      } else if (x == 0) {
        
         printf("Eixo Y\n");
    
        } else if (y == 0) {
       
           printf("Eixo X\n");
    
         } else if (x > 0 && y >= 1) {
    
            printf("Quadrante 1\n");
    
          } else if (x < 0 && y >= 1) {
        
             printf("Quadrante 2\n");
    
           } else if (x < 0 && y < 0) {
        
              printf("Quadrante 3\n");
    
            } else {
        
               printf("Quadrante 4\n");
    }

    return 0;
}
