#include <stdio.h>

main() {
    
    int np; 
    int i;

    printf("Digite um numero positivo sem casas decimais: ");
    scanf("%d", &np);

    printf("Esses sao os divisores do numero %d: ", n);

    for (i = 1; i <= n; i++) {
        
        if (np % i == 0) {
            
            printf("%d ", i);
        }
