#include <stdio.h>

int Total(int A, int B) {
    if (A > B) {
        return A;
    } else {
        return B;
    }
}

int main() {
    int n1, n2;
    printf("Informe dois numeros: ");
    scanf("%d %d", &n1, &n2);
    int MaiorN = Total(n1, n2);
    printf("O maior numero entre os dois e %d.\n", MaiorN);
    return 0;
}
